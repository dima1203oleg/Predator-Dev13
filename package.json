{
  "name": "predator-analytics-v13-frontend",
  "version": "13.0.0",
  "private": true,
  "description": "React Nexus Core + OpenWebUI frontend for Predator Analytics v13",
  "scripts": {
    "dev": "concurrently \"npm run dev:nexus\" \"npm run dev:openwebui\"",
    "dev:nexus": "cd frontend/nexus-core && npm run dev",
    "dev:openwebui": "cd frontend/openwebui && npm run dev",
    "build": "npm run build:nexus && npm run build:openwebui",
    "build:nexus": "cd frontend/nexus-core && npm run build",
    "build:openwebui": "cd frontend/openwebui && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "workspaces": [
    "frontend/nexus-core",
    "frontend/openwebui",
    "frontend/dashboard"
  ],
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
